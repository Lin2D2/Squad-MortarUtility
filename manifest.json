{
    "manifest_version":1,
    "type":"WebApp",
    "meta":{
        "name":"Squad Mortar Utility",
        "version":"1.0.0",
        "minimum-overwolf-version":"0.77.10",
        "author":"Lin2D2",
        "icon":"icon.jpg",
        "icon_gray":"icon.jpg",
        "description":"Squad Mortar Utility"
    },
    "data": {
        "game_targeting": {"type": "dedicated","game_ids": []},
        "start_window":"InGame",
        "windows":{
            "Background":{
                "file":"windows/background/index.html",
                "is_background_page": true
            },
            "InGame":{
                    "file":"windows/inGame/index.html",
                    "transparent": false,
                    "resizable": false,
                    "use_os_windowing": false,
                    "in_game_only": true,
                    "restrict_to_game_bounds": true,
                    "topmost": true,
                    "grab_keyboard_focus": false,
                    "start_position": {
                        "top": 50,
                        "left": 15
                    },
                    "size": {
                        "width":240,
                        "height":290
                        }
                }
        }
    }
}
